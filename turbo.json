{
    "$schema": "https://turbo.build/schema.json",
    "globalDotEnv": [".env.local", ".env"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
          },
          "deploy": {
            "dependsOn": ["build", "test", "lint"]
          }, 
          "lint": {}
    }
}