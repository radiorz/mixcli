{
  "name": "mixcli-repos",
  "version": "1.0.0",
  "private": true,
  "description": "Create CommandLine Application with Interactive prompts",
  "homepage": "https://zhangfisher.github.io/mixcli/",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "pnpm --filter mixcli build",
    "release": "nrm use npm && cd packages/cli && pnpm build && npm publish --access public && cd ../.. && nrm use taobao && cnpm sync mixcli",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "sync": "cnpm sync mixcli",
    "changeset": "changeset && npx changeset version && copy packages\\cli\\CHANGELOG.md docs\\CHANGELOG.md"
  },
  "keywords": [],
  "author": "",
  "license": "ISC", 
    "dependencies": {
      "art-template": "^4.13.2",
      "commander": "^11.0.0",
      "flex-tools": "^1.3.27",
      "fs-extra": "^11.1.1",
      "logsets": "^1.3.7"
    },
    "devDependencies": {
      "@changesets/cli": "^2.26.2",
      "@types/fs-extra": "^11.0.1",
      "@types/node": "^20.5.7",
      "tsup": "^7.2.0",
      "typescript": "^5.2.2",
      "vitepress": "1.0.0-rc.10",
      "vue": "^3.3.4"
    }
  }